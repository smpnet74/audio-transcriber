# Copy this file to .env and add your actual API keys and model settings

# Groq API Configuration
GROQ_API_KEY=gsk_your_api_key_here

# Speech-to-Text Model Configuration
SPEECH_MODEL=whisper-large-v3-turbo

# LLM Analysis Model Configuration  
ANALYSIS_MODEL=moonshotai/kimi-k2-instruct

# Optional: Enable transcript correction (true/false)
ENABLE_TRANSCRIPT_CORRECTION=true

# Optional: Enable detailed analysis (true/false)
ENABLE_DETAILED_ANALYSIS=true
